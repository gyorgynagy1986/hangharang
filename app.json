{
  "expo": {
    "name": "Hangharang Kezel≈ë",
    "slug": "hangharang-app",
    "version": "1.0.6",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "hangharangapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.hangharang"
    },
    "android": {
      "package": "com.yourname.hangharang",
      "versionCode": 6,
      "permissions": ["VIBRATE", "WAKE_LOCK", "SYSTEM_ALERT_WINDOW"],
      "adaptiveIcon": {
        "backgroundColor": "#1a1a2e",
        "foregroundImage": "./assets/images/android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1a1a2e",
          "dark": {
            "backgroundColor": "#1a1a2e"
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9eb10498-ce98-45e2-813e-387cd67e7ab1"
      }
    }
  }
}
